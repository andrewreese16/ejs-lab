<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title><%= category %>Menu</title>
  </head>
  <body>
    <%- include('./partials/nav') %>

    <h1><%= category %> Menu</h1>
    <ul>
      <% menuItems.forEach(item => { %>
      <li>
        <%= item.name %><br />
        Price: $<%= item.price %><br />
        Rating: <%= item.rating %>stars<br />
        Details: <%= item.details %>
      </li>
      <% }) %>
    </ul>
  </body>
</html>
